{
  "name": "contributte/neonizer",
  "description": "Collection of useful scripts for manipulaton with NEON files. Support composer pre-install / post-install events. Set/get parameters, validate files.",
  "keywords": [
    "nette",
    "composer",
    "parameters",
    "neon",
    "interactive"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/neonizer",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nette/neon": "^3.0.0"
  },
  "require-dev": {
    "composer/composer": "^1.6.3|^2.0",
    "mockery/mockery": "^1.3.0",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Neonizer\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
